{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "VSCode Dark Modern Like",
  "author": "Mohammadreza Amani",
  "themes": [
    {
      "name": "VSCode Dark Modern",
      "appearance": "dark",
      "style": {
        "border": "#333333ff",
        "border.variant": "#2a2a2aff",
        "border.focused": "#007accff",
        "border.selected": "#094771ff",
        "border.transparent": "#00000000",
        "border.disabled": "#383838ff",

        "elevated_surface.background": "#2d2d2dff",
        "surface.background": "#252526ff",
        "background": "#252526ff",

        "element.background": "#252526ff",
        "element.hover": "#3c3c3cff",
        "element.active": "#094771ff",
        "element.selected": "#094771ff",
        "element.disabled": "#303031ff",

        "drop_target.background": "#007acc80",

        "ghost_element.background": "#00000000",
        "ghost_element.hover": "#3c3c3cff",
        "ghost_element.active": "#094771ff",
        "ghost_element.selected": "#094771ff",
        "ghost_element.disabled": "#303031ff",

        "text": "#ccccccff",
        "text.muted": "#808080ff",
        "text.placeholder": "#808080ff",
        "text.disabled": "#666666ff",
        "text.accent": "#3794ffff",

        "icon": "#ccccccff",
        "icon.muted": "#808080ff",
        "icon.disabled": "#666666ff",
        "icon.placeholder": "#808080ff",
        "icon.accent": "#3794ffff",

        "status_bar.background": "#252526ff",
        "title_bar.background": "#2d2d2dff",
        "title_bar.inactive_background": "#252526ff",
        "toolbar.background": "#252526ff",

        "tab_bar.background": "#252526ff",
        "tab.inactive_background": "#2d2d2dff",
        "tab.active_background": "#1e1e1eff",

        "search.match_background": "#515c6a7e",
        "search.current_match_background": "#007acc60",

        "panel.background": "#252526ff",
        "panel.focused_border": "#007accff",

        "pane.focused_border": "#007accff",

        "scrollbar.thumb.background": "#42424280",
        "scrollbar.thumb.hover_background": "#555555a0",
        "scrollbar.thumb.border": "#00000000",
        "scrollbar.track.background": "#00000000",
        "scrollbar.track.border": "#00000000",

        "editor.foreground": "#d4d4d4ff",
        "editor.background": "#1e1e1eff",
        "editor.gutter.background": "#1e1e1eff",
        "editor.subheader.background": "#252526ff",
        "editor.active_line.background": "#2a2a2aff",
        "editor.highlighted_line.background": "#2a2d2e80",

        "editor.line_number": "#858585ff",
        "editor.active_line_number": "#c6c6c6ff",
        "editor.hover_line_number": "#d4d4d4ff",

        "editor.invisible": "#3b3b3bff",
        "editor.wrap_guide": "#404040ff",
        "editor.active_wrap_guide": "#505050ff",

        "editor.document_highlight.read_background": "#264f784c",
        "editor.document_highlight.write_background": "#52545580",

        "terminal.background": "#1e1e1eff",
        "terminal.foreground": "#ccccccff",
        "terminal.bright_foreground": "#ffffff",
        "terminal.dim_foreground": "#cccccc99",
        "terminal.ansi.black": "#000000ff",
        "terminal.ansi.bright_black": "#666666ff",
        "terminal.ansi.dim_black": "#000000b3",
        "terminal.ansi.red": "#cd3131ff",
        "terminal.ansi.bright_red": "#f14c4cff",
        "terminal.ansi.dim_red": "#cd3131b3",
        "terminal.ansi.green": "#0dbc79ff",
        "terminal.ansi.bright_green": "#23d18bff",
        "terminal.ansi.dim_green": "#0dbc79b3",
        "terminal.ansi.yellow": "#e5e510ff",
        "terminal.ansi.bright_yellow": "#f5f543ff",
        "terminal.ansi.dim_yellow": "#e5e510b3",
        "terminal.ansi.blue": "#3b8eea",
        "terminal.ansi.bright_blue": "#5ea6faff",
        "terminal.ansi.dim_blue": "#3b8eeab3",
        "terminal.ansi.magenta": "#bc3fbcff",
        "terminal.ansi.bright_magenta": "#d670d6ff",
        "terminal.ansi.dim_magenta": "#bc3fbcb3",
        "terminal.ansi.cyan": "#11a8cdff",
        "terminal.ansi.bright_cyan": "#29b8dbff",
        "terminal.ansi.dim_cyan": "#11a8cdb3",
        "terminal.ansi.white": "#e5e5e5ff",
        "terminal.ansi.bright_white": "#e5e5e5ff",
        "terminal.ansi.dim_white": "#e5e5e5b3",

        "link_text.hover": "#3794ffff",

        "version_control.added": "#81b88bff",
        "version_control.modified": "#e2c08dff",
        "version_control.deleted": "#c74e39ff",
        "version_control.renamed": "#73b8ff",
        "version_control.ignored": "#80808080",

        "created": "#81b88bff",
        "created.background": "#81b88b1a",
        "created.border": "#6d9f78ff",
        "deleted": "#c74e39ff",
        "deleted.background": "#c74e391a",
        "deleted.border": "#a14134ff",
        "error": "#f44747ff",
        "error.background": "#f447471a",
        "error.border": "#e45959ff",
        "hidden": "#808080ff",
        "hidden.background": "#8080801a",
        "hidden.border": "#6a6a6aff",
        "hint": "#6c9ef8ff",
        "hint.background": "#6c9ef81a",
        "hint.border": "#567cc3ff",
        "ignored": "#80808080",
        "ignored.background": "#8080801a",
        "ignored.border": "#6a6a6aff",
        "info": "#3794ffff",
        "info.background": "#3794ff1a",
        "info.border": "#2a75c7ff",
        "modified": "#e2c08dff",
        "modified.background": "#e2c08d1a",
        "modified.border": "#bda371ff",
        "predictive": "#80808080",
        "predictive.background": "#80808015",
        "predictive.border": "#6a6a6aff",
        "renamed": "#73b8ffff",
        "renamed.background": "#73b8ff1a",
        "renamed.border": "#5a93caff",
        "success": "#81b88bff",
        "success.background": "#81b88b1a",
        "success.border": "#6d9f78ff",
        "unreachable": "#808080ff",
        "unreachable.background": "#8080801a",
        "unreachable.border": "#6a6a6aff",
        "warning": "#cca700ff",
        "warning.background": "#cca7001a",
        "warning.border": "#ad8d00ff",
        "conflict": "#cca700ff",
        "conflict.background": "#cca7001a",
        "conflict.border": "#ad8d00ff",

        "players": [
          { "cursor": "#3794ffff", "selection": "#264f78ff" },
          { "cursor": "#d670d6ff", "selection": "#bc3fbc5a" },
          { "cursor": "#23d18bff", "selection": "#0dbc795a" },
          { "cursor": "#f5f543ff", "selection": "#e5e5105a" },
          { "cursor": "#f14c4cff", "selection": "#cd31315a" },
          { "cursor": "#ff8c00ff", "selection": "#cc70005a" },
          { "cursor": "#29b8dbff", "selection": "#11a8cd5a" },
          { "cursor": "#b0b0b0ff", "selection": "#8080805a" }
        ],

        "syntax": {
          "attribute": { "color": "#9cdcfeff" },
          "boolean": { "color": "#569cd6ff" },
          "comment": { "color": "#6a9955ff" },
          "comment.doc": { "color": "#608b4eff" },
          "constant": { "color": "#9cdcfeff" },
          "constructor": { "color": "#dcdcaa", "font_style": "italic" },
          "embedded": { "color": "#d4d4d4ff" },
          "emphasis": { "color": "#d4d4d4ff", "font_style": "italic" },
          "emphasis.strong": { "color": "#d4d4d4ff", "font_weight": 700 },
          "enum": { "color": "#4ec9b0ff" },
          "function": { "color": "#dcdcaa" },
          "hint": { "color": "#6c9ef8b3" },
          "keyword": { "color": "#569cd6ff" },
          "label": { "color": "#c586c0ff" },
          "link_text": { "color": "#ce9178ff", "font_style": "italic" },
          "link_uri": { "color": "#6a9955ff", "font_style": "italic" },
          "number": { "color": "#b5cea8ff" },
          "operator": { "color": "#d4d4d4ff" },
          "predictive": { "color": "#80808080", "font_style": "italic" },
          "preproc": { "color": "#c586c0ff" },
          "primary": { "color": "#d4d4d4ff" },
          "property": { "color": "#9cdcfeff" },
          "punctuation": { "color": "#d4d4d4ff" },
          "punctuation.bracket": { "color": "#d4d4d4ff" },
          "punctuation.delimiter": { "color": "#d4d4d4ff" },
          "punctuation.list_marker": { "color": "#d4d4d4ff" },
          "punctuation.special": { "color": "#c586c0ff" },
          "string": { "color": "#ce9178ff" },
          "string.escape": { "color": "#d7ba7dff" },
          "string.regex": { "color": "#d16969ff" },
          "string.special": { "color": "#ce9178ff" },
          "string.special.symbol": { "color": "#ce9178ff" },
          "tag": { "color": "#569cd6ff" },
          "text.literal": { "color": "#ce9178ff" },
          "title": { "color": "#dcdcaa", "font_weight": 700 },
          "type": { "color": "#4ec9b0ff" },
          "variable": { "color": "#9cdcfeff" },
          "variable.special": { "color": "#569cd6ff" },
          "variant": { "color": "#4ec9b0ff" }
        }
      }
